<?xml version="1.0"?>
<block>
  <name>MAX2769 Source</name>
  <key>gnmax2769_source_cc</key>
  <category>Sources</category>
  <throttle>1</throttle>
  <import>import gnMAX2769</import>
  <make>gnMAX2769.source_cc($bias,$ant,$freq,$bw)</make>
  <callback>set_bias($bias)</callback>
  <callback>set_ant($ant)</callback>
  <callback>set_freq($freq)</callback>
  <callback>set_bw($bw)</callback>
  <param>
    <name>Bias Voltage</name>
    <key>bias</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>Off</name>
      <key>0</key>
    </option>
    <option>
      <name>On</name>
      <key>1</key>
    </option>
  </param>
  <param>
    <name>Antenna Select</name>
    <key>ant</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>Auto</name>
      <key>0</key>
    </option>
    <option>
      <name>Ant 1</name>
      <key>1</key>
    </option>
    <option>
      <name>Ant 2</name>
      <key>2</key>
    </option>
    <option>
      <name>Off</name>
      <key>3</key>
    </option>
  </param>
  <param>
    <name>Frequency (Hz)</name>
    <key>freq</key>
    <value>1575420e3</value>
    <type>real</type>
  </param>
  <param>
    <name>Bandwidth</name>
    <key>bw</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>2.5MHz</name>
      <key>0</key>
    </option>
    <option>
      <name>4.2MHz</name>
      <key>2</key>
    </option>
    <option>
      <name>8MHz</name>
      <key>1</key>
    </option>
    <option>
      <name>18MHz</name>
      <key>3</key>
    </option>
  </param>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
